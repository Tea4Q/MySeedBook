{
  "expo": {
    "name": "MySeedBook Catalogue",
    "slug": "myseedbook-catalogue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myseedbook-catalogue",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "A comprehensive gardening application for managing your seed collection and supplier information. Track planting seasons, harvest times, and maintain detailed records of your gardening inventory.",
    "primaryColor": "#336633",
    "splash": {
      "image": "./assets/images/splashScreenImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f9f0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myseedbook.catalogue",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.myseedbook.catalogue",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#f0f9f0"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow MySeedBook Catalogue to access your camera to capture photos of your seeds and plants."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow MySeedBook Catalogue to access your photo library to select images of your seeds and plants."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "debc61fc-b3ac-46ad-9220-6c452667b7e3"
      }
    }
  }
}
